{
  "manifest_version": 2,
  "name": "Frame css reloader",
  "description": "Frame css reloader",
  "version": "1.0.0",
  "icons": {
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "tabs"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["background.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  }
}
